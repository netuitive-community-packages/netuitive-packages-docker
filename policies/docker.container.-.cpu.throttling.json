{
  "policy": {
    "actions": [
      {
        "category": 2,
        "type": "event"
      }
    ],
    "conditions": [
      {
        "analytic": "actual",
        "level": 0,
        "metric": "netuitive.docker.cpu.container_throttling_percent",
        "operator": ">"
      }
    ],
    "deleted": false,
    "description": "The Docker container has had its CPU usage throttled for at least the past 15 minutes.",
    "duration": 900,
    "enabled": true,
    "name": "Docker Container - CPU Throttling",
    "scope": {
      "elementTypes": [
        "Docker Container"
      ]
    }
  }
}